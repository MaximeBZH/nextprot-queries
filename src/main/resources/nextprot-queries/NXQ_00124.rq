#id:NXQ_00124
#title:What are the 25 most frequent families with member count
#tags:snorql-only,family,tutorial
#acs:
#count:25
#time:0.5

select str(?famlab) (count(distinct(?member)) as ?membercnt) where{
  ?member :family /:term /:childOf / rdfs:label ?famlab .
} group by ?famlab
order by desc(?membercnt)
limit 25
